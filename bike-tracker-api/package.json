{
  "name": "bike-tracker-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "generate:openapi": "curl -s http://localhost:8787/doc > ../openapi.json",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply bike-tracker-db --local",
    "db:migrate:remote": "wrangler d1 migrations apply bike-tracker-db --remote",
    "test": "vitest"
  },
  "dependencies": {
    "@bike-tracker/shared": "*",
    "@hono/swagger-ui": "^0.5.3",
    "@hono/zod-openapi": "^1.2.2",
    "@hono/zod-validator": "^0.7.6",
    "drizzle-orm": "^0.36.0",
    "hash-wasm": "^4.12.0",
    "hono": "^4.6.0",
    "jose": "^5.9.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260214.0",
    "drizzle-kit": "^0.28.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0",
    "wrangler": "^3.91.0"
  }
}
